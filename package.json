{
  "version": "1.0.0",
  "name": "buffet",
  "main": "dist/index",
  "license": "MIT",
  "scripts": {
    "build": "cross-env rimraf dist && babel src --out-dir dist --copy-files --no-comments && rimraf dist/**/**/tests",
    "build-storybook": "build-storybook",
    "lint": "eslint --cache src .storybook internals stories",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint src/ .storybook/ stories/ internals/ --fix",
    "generate": "plop --plopfile internals/generators/index.js",
    "storybook": "start-storybook -p 3000",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:update": "cross-env NODE_ENV=test jest -u"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "yarn run lint:fix",
      "yarn run lint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "lodash": "^4.17.11",
    "prop-types": "^15.5.10",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.2.1",
    "cross-env": "^5.1.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "plop": "^2.2.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.2.1"
  }
}
